@model IEnumerable<TaskInsider.InsiderPhoto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PhotoFileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Insider.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Insider.InsiderFirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Insider.InsiderLastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <img src="@Url.Content(item.PhotoFileName)" alt="Image Is Missing" />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Insider.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Insider.InsiderFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Insider.InsiderLastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.InsiderPhotoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.InsiderPhotoID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.InsiderPhotoID })
        </td>
    </tr>
}

</table>
